{
  "bindings": [
    {
      "queueName": "facebook_media_store",
      "name": "message",
      "type": "serviceBusTrigger",
      "direction": "in",
      "connection": "AzureWebJobsServiceBus",
      "accessRights": "Manage"
    },
    {
      "type": "documentDB",
      "name": "facebookmedia",
      "databaseName": "reporting",
      "collectionName": "media",
      "createIfNotExists": true,
      "connection": "DocumentDB",
      "direction": "out"
    }
  ],
  "disabled": false
}