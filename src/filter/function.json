{
  "bindings": [
    {
      "queueName": "tofilter",
      "name": "message",
      "type": "serviceBusTrigger",
      "direction": "in",
      "connection": "AzureWebJobsServiceBus",
      "accessRights": "Manage"
    },
    {
      "name": "userdata",
      "type": "documentDB",
      "direction": "in",
      "connection": "DocumentDB",
      "databaseName": "mean-dev",
      "collectionName": "users",
      "sqlQuery": "SELECT * FROM c WHERE c.twitter['$id'] = {userid} AND 'twitter' = {platform}"
    },
    {
      "type": "serviceBus",
      "name": "out",
      "topicName": "toaugment",
      "connection": "AzureWebJobsServiceBus",
      "accessRights_": "Manage",
      "direction": "out"
    }
  ],
  "disabled": false
}