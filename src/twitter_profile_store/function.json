{
  "bindings": [
    {
      "queueName": "twitter_profile_store",
      "name": "message",
      "type": "serviceBusTrigger",
      "direction": "in",
      "connection": "AzureWebJobsServiceBus",
      "accessRights": "Manage"
    },
    {
      "type": "documentDB",
      "name": "twitterprofile",
      "databaseName": "reporting",
      "collectionName": "profile",
      "createIfNotExists": true,
      "connection": "DocumentDB",
      "direction": "out"
    }
  ],
  "disabled": false
}